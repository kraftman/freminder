extends _bootstrap
block body
  .navbar.navbar-default
    .container-fluid
      .navbar-header
        a.navbar-brand(href="#") test
        ul.nav.navbar-nav
          li.active: a(href='/test') test1
      #collapse1.collapse.navbar-collapse
        if uid
          ul.nav.navbar-nav.navbar-right
            li.active: a(href='/logout') logout
        else
          form(method='POST' action = '/sendtoken')
            ul.nav.navbar-nav.navbar-right.navbar-form
              .input-group()
                +input-simple("text","txtName",{name: 'user',placeholder: 'email'})
          
          
  +accordion("sample")
    each val in [1, 2, 3, 4, 5]
      +accordion-item-default("Title1"+val,"sample")
        |  #{val} This is first accordion content
